<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.insta.clone.mapper.TagMapper">
    <insert id="upload" parameterType="com.insta.clone.dto.TagDTO"  useGeneratedKeys="true"
            keyProperty="id">
        INSERT INTO tag(tag_name, image_id) VALUES (#{tagName}, #{imageId})
    </insert>
</mapper>